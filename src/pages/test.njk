---
layout: "layouts/base.njk"
permalink: "/test/index.html"
---

{% for year, results in results | groupby("year") | dictsort | reverse %}
  <details{{ ' open' if loop.first }}>
    <summary>
      {{ year }}
    </summary>
    <table>
      <thead>
        <th>Pos</th>
        <th>Name</th>
        <th>Time</th>
      </thead>
      <tbody>
        {% for result in results | sort(attribute="position") %}
          <tr>
            <td>
              {{ result.position }}
            </td>
            <td>
              {{ result.name }}
            </td>
            <td>
              {{ result.time }}
            </td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </details>
{% endfor %}
